{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-typescript",
    [
      "airbnb",
      {
        "targets": {
          "node": 4
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-flow-strip-types",
    "add-module-exports"
  ]
}
