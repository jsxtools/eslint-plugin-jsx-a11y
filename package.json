    "lib"
  ],
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "a11y",
    "accessibility",
    "jsx"
  ],
  "author": "Ethan Cohen",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsxtools/eslint-plugin-jsx-a11y"
  },
  "scripts": {
    "prepack": "npmignore --auto --commentLines=autogenerated && npm run build",
    "build": "node scripts/build.js",
    "create": "node ./scripts/create-rule",
    "lint:fix": "npm run lint -- --fix",
    "lint": "eslint --ext=js,mjs,cjs,ts,tsx .",
    "prepublish": "not-in-publish || npm run prepublishOnly",
    "prepublishOnly": "safe-publish-latest && npm run lint && npm run jest",
    "pretest": "npm run lint:fix",
    "test": "npm run jest",
    "posttest": "aud --production",
    "test:ci": "npm run jest -- --ci --runInBand",
    "jest": "jest --coverage __tests__/**/*",
    "pregenerate-list-of-rules": "npm run build",
    "generate-list-of-rules": "eslint-doc-generator --rule-doc-title-format prefix-name --rule-doc-section-options false --config-emoji recommended,☑️",
    "generate-list-of-rules:check": "npm run generate-list-of-rules -- --check",
    "version": "auto-changelog && git add CHANGELOG.md",
    "postversion": "auto-changelog && git add CHANGELOG.md && git commit --no-edit --amend && git tag -f \"v$(node -e \"console.log(require('./package.json').version)\")\""
  },
  "engines": {
    "node": ">=4.0"
  },
  "license": "MIT",
  "dependencies": {
    "aria-query": "^5.3.0",
    "axe-core": "=4.7.0",
    "axobject-query": "^3.2.1",
    "damerau-levenshtein": "^1.0.8",
    "emoji-regex": "^9.2.2",
    "es-iterator-helpers": "^1.0.15",
    "jsx-ast-utils": "^3.3.5",
    "language-tags": "^1.0.9",
    "minimatch": "^3.1.2",
    "safe-regex-test": "^1.0.2"
  },
  "peerDependencies": {
    "eslint": "^3 || ^4 || ^5 || ^6 || ^7 || ^8"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-typescript": "^7.24.1",
    "@babel/register": "^7.23.7",
    "@types/eslint": "^8.56.6",
    "@types/estree": "^1.0.5",
    "@types/minimatch": "^5.1.2",
    "@types/safe-regex-test": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "aud": "^2.0.4",
    "auto-changelog": "^2.4.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-preset-airbnb": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-doc-generator": "^1.6.1",
    "eslint-plugin-eslint-plugin": "^4.3.0",
    "eslint-plugin-flowtype": "^5.8.0 || ^8.0.3",
    "eslint-plugin-import": "^2.29.1",
    "estraverse": "^5.3.0",
    "estree-jsx": "^0.0.1",
    "expect": "^24.9.0",
    "in-publish": "^2.0.1",
    "jackspeak": "=2.1.1",
    "jest": "^24.9.0",
    "jscodeshift": "^0.7.1",
    "minimist": "^1.2.8",
    "npmignore": "^0.3.1",
    "object.assign": "^4.1.5",
    "rimraf": "^3.0.2",
    "safe-publish-latest": "^2.0.0",
    "semver": "^6.3.1",
    "to-ast": "^1.0.0",
    "typescript": "^5.4.3",
    "typescript-eslint": "^7.4.0",
    "vite": "^5.2.6"
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "json",
      "html"
    ],
    "coverageDirectory": "coverage",
    "roots": [
      "__tests__"
    ],
    "testPathIgnorePatterns": [
      "__tests__/__util__/"
    ],
    "testEnvironment": "node"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": false,
    "commitLimit": false,
    "backfillLimit": false,
    "hideCredit": true,
    "startingVersion": "6.6.2"
  },
  "publishConfig": {
    "ignore": [
      "!lib",
      ".github/workflows",
      ".flowconfig",
      "/src",
      "/reports",
      "/flow",
      "scripts/",
      "CONTRIBUTING.md"
    ]
  }
}
